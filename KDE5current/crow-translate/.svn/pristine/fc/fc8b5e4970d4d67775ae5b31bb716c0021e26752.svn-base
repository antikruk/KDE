#: data/org.kde.CrowTranslate.desktop.in:3
msgctxt "Name"
msgid "Crow Translate"
msgstr "Crow Translate"

#: data/org.kde.CrowTranslate.desktop.in:21
msgctxt "GenericName"
msgid "Translator"
msgstr "Перакладчык"

#: data/org.kde.CrowTranslate.desktop.in:40
msgctxt "Comment"
msgid ""
"A simple and lightweight translator that allows to translate and speak the "
"selected text using Mozhi"
msgstr ""
"Просты і лёгкаважны перакладчык, які можа перакладаць і вымаўляць абраны "
"тэкст з дапамогай Mozhi"

#: data/org.kde.CrowTranslate.desktop.in:58
msgctxt "Keywords"
msgid "google;yandex;deepl;duckduckgo;libre';mymemory;reverso;text-to-speech;"
msgstr "google;yandex;deepl;duckduckgo;libre';mymemory;reverso;text-to-speech;"

#: data/org.kde.CrowTranslate.desktop.in:88
msgctxt "Name"
msgid "Translate selected text"
msgstr "Перакласці абраны тэкст"

#: data/org.kde.CrowTranslate.desktop.in:110
msgctxt "Name"
msgid "Speak selected text"
msgstr "Вымавіць абраны тэкст"

#: data/org.kde.CrowTranslate.desktop.in:132
msgctxt "Name"
msgid "Speak translation of selected text"
msgstr "Вымавіць пераклад абранага тэксту"

#: data/org.kde.CrowTranslate.desktop.in:154
msgctxt "Name"
msgid "Stop text speaking"
msgstr "Спыніць вымаўленне тэксту"

#: data/org.kde.CrowTranslate.desktop.in:176
msgctxt "Name"
msgid "Show main window"
msgstr "Паказаць галоўнае акно"

#: data/org.kde.CrowTranslate.desktop.in:199
msgctxt "Name"
msgid "Translate selected text and copy to clipboard"
msgstr "Перакласці абраны тэкст і скапіяваць у буфер абмену"

#: data/org.kde.CrowTranslate.desktop.in:220
msgctxt "Name"
msgid "Recognize text in screen area"
msgstr "Распазнаць тэкст у вобласці экрана"

#: data/org.kde.CrowTranslate.desktop.in:242
msgctxt "Name"
msgid "Translate text in screen area"
msgstr "Перакласці тэкст у вобласці экрана"
