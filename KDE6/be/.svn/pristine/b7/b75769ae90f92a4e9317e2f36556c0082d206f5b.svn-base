#: src/kcmplymouth_actions.actions:2
msgctxt "Name"
msgid "Plymouth Control Module"
msgstr "Модуль кіравання Plymouth"

#: src/kcmplymouth_actions.actions:53
msgctxt "Name"
msgid "Save the Plymouth Theme"
msgstr "Захаваць тэму Plymouth"

#: src/kcmplymouth_actions.actions:101
msgctxt "Description"
msgid "Authenticate to change the boot splash screen."
msgstr "Каб змяніць застаўку падчас запуску, выканайце аўтэнтыфікацыю."

#: src/kcmplymouth_actions.actions:140
msgctxt "Name"
msgid "Install a new Plymouth Theme"
msgstr "Усталяваць новую тэму Plymouth"

#: src/kcmplymouth_actions.actions:188
msgctxt "Description"
msgid "Authenticate to install a Plymouth theme."
msgstr "Каб усталяваць тэму Plymouth, выканайце аўтэнтыфікацыю."

#: src/kcmplymouth_actions.actions:227
msgctxt "Name"
msgid "Uninstall a new Plymouth Theme"
msgstr "Выдаліць новую тэму Plymouth"

#: src/kcmplymouth_actions.actions:275
msgctxt "Description"
msgid "Authenticate to uninstall a Plymouth theme."
msgstr "Каб выдаліць тэму Plymouth, выканайце аўтэнтыфікацыю."

#: src/plymouth.knsrc:2
msgctxt "Name"
msgid "Boot Splash Screens"
msgstr "Застаўкі экрана загрузкі"

#~ msgctxt "Description"
#~ msgid "System policies prevent you from installing a Plymouth theme."
#~ msgstr "Сістэмныя правілы не дазваляюць усталяванне тэмы Plymouth."

#~ msgctxt "Description"
#~ msgid "System policies prevent you from applying the Plymouth theme."
#~ msgstr "Сістэмныя правілы не дазваляюць ужыць тэму Plymouth."
