#| msgid "Show Filter Bar"
msgctxt "@action:inmenu"
msgid "Toggle Search Bar"
msgstr "Паказаць або схаваць панэль пошуку"

#: dolphinmainwindow.cpp:1664
#| msgctxt "@title:menu"
#| msgid "Main Toolbar"
msgctxt "@action:intoolbar"
msgid "Search"
