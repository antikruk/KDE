msgstr ""

#: kfmclient.cpp:80
#, kde-format
msgid "\n"
"Syntax:\n"
msgstr "\n"
"Сінтаксіс:\n"

#: kfmclient.cpp:81
#, kde-format
msgid "  kfmclient openURL 'url' ['mimetype']\n"
"            # Opens a window showing 'url'.\n"
"            #  'url' may be a relative path\n"
"            #   or file name, such as . or subdir/\n"
"            #   If 'url' is omitted, $HOME is used instead.\n\n"
msgstr "  kfmclient openURL 'url' ['mimetype']\n"
"            # Адкрывае акно, якое паказвае 'url'.\n"
"            #  'url' можа быць адносным шляхам\n"
"            #   ці назвай файлу, напрыклад s . ці subdir/\n"
"            #   Калі 'url' апушчаны, адкрываецца $HOME.\n\n"

#: kfmclient.cpp:86
#, kde-format
msgid "            # If 'mimetype' is specified, it will be used to determine the\n"
"            #   component that Konqueror should use. For instance, set it to\n"
"            #   text/html for a web page, to make it appear faster\n\n"
msgstr "            # Калі 'mimetype' зададзены, ён будзе выкарыстаны каб вызначыць\n"
"            #   кампанент, якім Konqueror будзе карыстацца. На прыклад, выстаўце яго як\n"
"            #   text/html для web-старонкі, каб яна з'яўлалася хутчэй\n\n"

#: kfmclient.cpp:90
#, kde-format
msgid "  kfmclient newTab 'url' ['mimetype']\n"
"            # Same as above but opens a new tab with 'url' in an existing Konqueror\n"
"            #   window on the current active desktop if possible.\n\n"
msgstr ""

